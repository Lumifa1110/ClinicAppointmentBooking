<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity.DoctorHomePage">
  <FrameLayout
      android:id="@+id/container"
      android:layout_width="match_parent"
      android:layout_height="0dp"
      app:layout_constraintTop_toTopOf="parent"
      android:layout_marginBottom="20dp"
      app:layout_constraintBottom_toTopOf="@+id/doctorNavBar"/>
  <com.google.android.material.bottomnavigation.BottomNavigationView
      android:id="@+id/doctorNavBar"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:layout_marginStart="20dp"
      android:layout_marginEnd="20dp"
      android:layout_marginBottom="10dp"
      android:background="@drawable/navbar_rounded"
      app:itemIconTint="@color/nav_items_color"
      app:itemTextColor="@color/nav_items_color"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintRight_toRightOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:menu="@menu/doctor_menu" />
</androidx.constraintlayout.widget.ConstraintLayout>

